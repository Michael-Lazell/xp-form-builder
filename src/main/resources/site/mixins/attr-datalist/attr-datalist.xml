<mixin>
  <display-name>Attr: Datalist</display-name>
  <items>
    <item-set name="datalist">
      <label>Datalist</label>
      <items>
        <item-set name="datalistOptions">
          <label>Option</label>
          <items>
            <input name="optionLabel" type="TextLine">
              <label>Label</label>
              <occurrences minimum="1" maximum="1"/>
            </input>
            <input name="optionValue" type="TextLine">
              <label>Value</label>
              <occurrences minimum="1" maximum="1"/>
            </input>
          </items>
          <immutable>false</immutable>
          <occurrences minimum="1" maximum="0"/>
        </item-set>
      </items>
      <immutable>false</immutable>
      <occurrences minimum="0" maximum="1"/>
    </item-set>
  </items>
</mixin>